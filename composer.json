{
    "name": "xeops/iiko-exchange-bundle",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-ftp": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-ssh2": "*",
        "php-http/guzzle6-adapter": "^1.1",
        "symfony/config": "^5.3",
        "symfony/event-dispatcher": "^5.3",
        "symfony/monolog-bundle": "^3.7",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "phpunit/php-code-coverage": "^9.2",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "iikoExchangeBundle\\": "src/",
            "iikoExchangeBundle\\Library\\": "src/library"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    }
}
